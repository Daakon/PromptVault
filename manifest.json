{
  "manifest_version": 3,
  "name": "PromptVault",
  "version": "1.0",
  "description": "Organize and enhance your AI prompts in the side panel.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "permissions": [
    "sidePanel",
    "storage"
  ],
  "host_permissions": [
    "https://generativelanguage.googleapis.com/*"
  ]
}